{
  "name": "vue-el-element",
  "version": "1.12.8",
  "description": "Vue components for standard design elements",
  "author": "Mattias Naarttij√§rvi",
  "homepage": "https://elelement.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Naartti/vue-el-element.git"
  },
  "keywords": [
    "ui",
    "components",
    "elements",
    "clean",
    "vue",
    "javascript"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "unpkg": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "version": "echo $npm_package_version",
    "preversion": "npm run lint && npm run build",
    "postversion": "npm publish",
    "serve": "vue-cli-service serve",
    "clear:dist": "rimraf dist",
    "build": "npm run clear:dist && NODE_ENV=production rollup -c",
    "build:demo": "vue-cli-service build --dest demo",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "watch": "vue-cli-service test:unit --watch",
    "prod": "npm run lint && npm run build:demo && firebase deploy"
  },
  "dependencies": {
    "core-js": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.6.0",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "highlight.js": "^9.18.1",
    "less": "^3.11.1",
    "less-loader": "^4.1.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.7.2",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-less": "^1.1.2",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11"
  }
}
